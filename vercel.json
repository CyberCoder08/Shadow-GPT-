
{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.js",
      "methods": ["GET"]
    },
    {
      "src": "/query",
      "dest": "index.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
